{
    "name": "kubricklabs/env",
    "description": "enviroment variables parser for kubricklabs",
    "version": "0.1.0-alpha",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Bentsen",
            "email": "thomas@lindgreenit.dk"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-pdo": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "kubricklabs/storage": "^0.1.0"
    },
   "autoload": {
        "psr-4": { "kubricklabs\\base\\env\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "scripts": {
        "test": "phpunit"
    }, 
    "minimum-stability": "alpha"
}
